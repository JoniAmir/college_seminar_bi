<div class="jumbotron" id="index_page">
  	
	<div class="container">
  	<h2>דוחות להנאתך</h2>
	</div>


	<div class="content-holder container">
		
		<div class="row-fluid">
			<div class="span6 graph">
				<%= @bar= Gchart.bar(:data => [300, 100, 30, 200, 10, 100], :title => 'Noisy') 
				image_tag(@bar) %>
			</div>
			<div class="span6 graph">
				<%= @pie=Gchart.pie_3d(:data => [20, 35, 45], :title => 'Steamy')
				 image_tag(@pie) %>
			</div>
		</div>

		<div class="row-fluid">
			<div class="span6"></div>
			<div class="span6"></div>
		</div>

		<div class="row-fluid">
			<div class="span6 graph">
				<%= @line=Gchart.line(:data => [0, 40, 10, 70, 20], :title => 'Pretty')
				 image_tag(@line) %>
			</div>
			<div class="span6 graph">
			<%= @line=Gchart.line(:data => [0, 40, 10, 70, 20], :title => 'Smelly')
				 image_tag(@line) %>
			</div>
		</div>
	</div>
	
</div>
